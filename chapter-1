public class Ex3 {
    public static void main(String[] args) {
        System.out.print("area of rectangle = width*heihht=");
        System.out.println(4.5*7.9);
        System.out.print("perimeter of a rectangle = (width+hight)*2=");
        System.out.println((4.5+7.9)*2);
    }
}
