public class Ex4 {
    
    public static void main(String[] args) {
        int f1 = 0 ;

        f1  = f1 + (((24*60*60)/7)*365) - ((24*60*60)/13*365)+((24*60*60)/45*365) ;
        for(int x = 312032486;;x=x+f1 ){
            System.out.print("value =");
             System.out.println(x);
              System.out.print("\n");
        if(x==323151846){
            break;

                   }
        }
      

        
    }
    
}
